using System;

 class HelloWorld
{
    public static void Main(string[] args)
    {
        int num;
        Console.Write("Enter the Table:");
        num = Convert.ToInt32(Console.ReadLine());
        
        for (int i=1; i<=10; i++)
        { 
            Console.WriteLine(num + " * " + i + " = " + (num * i));
        }
        Console.WriteLine();
    }
  
}


